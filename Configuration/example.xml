<?xml version="1.0" encoding="UTF-8"?>
<Configuration Identifier="example">
	<Schemes>
		<Scheme Identifier="product">
			<ApplicationIdentifier>com.cyber1496.UnityBuilder</ApplicationIdentifier>
			<CompanyName>cyber1496</CompanyName>
			<ProductName>UnityBuilder</ProductName>
			<Development>false</Development>
			<IOS>
    			<ScriptingDefineSymbols></ScriptingDefineSymbols>
				<AppleDeveloperTeamID>453R6FNS54</AppleDeveloperTeamID>
			</IOS>
			<Android>
    			<ScriptingDefineSymbols></ScriptingDefineSymbols>
				<UseBuildAppBundle>true</UseBuildAppBundle>
				<KeystoreName>Configuration/example.keystore</KeystoreName>
				<!-- <KeystoreName>${HOME}/example.keystore</KeystoreName> $ {HOME} can be specified. -->
				<!-- <KeystoreName>../example.keystore</KeystoreName> You can specify a relative path. -->
                <KeystorePass>example</KeystorePass>
				<KeyaliasName>example</KeyaliasName>
                <KeyaliasPass>example</KeyaliasPass>
			</Android>
			<Deploygate>
                <Authorization>${HOME}/UnityBuilder.xml</Authorization>
			</Deploygate>
		</Scheme>
		<Scheme Identifier="development">
			<ApplicationIdentifier>com.cyber1496.UnityBuilder.development</ApplicationIdentifier>
			<CompanyName>cyber1496</CompanyName>
			<ProductName>[D]UnityBuilder</ProductName>
			<Development>true</Development>
			<IOS>
    			<ScriptingDefineSymbols>APP_DEBUG</ScriptingDefineSymbols>
				<AppleDeveloperTeamID>453R6FNS54</AppleDeveloperTeamID>
			</IOS>
			<Android>
    			<ScriptingDefineSymbols>APP_DEBUG</ScriptingDefineSymbols>
				<UseBuildAppBundle>false</UseBuildAppBundle>
				<KeystoreName>Configuration/example.keystore</KeystoreName>
                <KeystorePass>example</KeystorePass>
				<KeyaliasName>example</KeyaliasName>
                <KeyaliasPass>example</KeyaliasPass>
			</Android>
			<Deploygate>
                <Authorization>${HOME}/UnityBuilder.xml</Authorization>
			</Deploygate>
		</Scheme>
	</Schemes>
</Configuration>