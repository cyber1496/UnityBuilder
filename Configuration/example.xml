			</IOS>
			<Android>
    			<ScriptingDefineSymbols>APP_DEBUG</ScriptingDefineSymbols>
				<KeystoreName></KeystoreName>
                <KeystorePass></KeystorePass>
				<KeyaliasName></KeyaliasName>
